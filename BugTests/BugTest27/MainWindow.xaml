<Window x:Class="BugTest27.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BugTest27"
        xmlns:ws="https://schemas.elecho.dev/wpfsuite"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ws:ListBox Margin="22">
            <ws:ListBox.ItemContainerStyle>
                <Style TargetType="ws:ListBoxItem">
                    <Setter Property="Padding" Value="10"/>
                    <Setter Property="Background" Value="#529cbd"/>
                </Style>
            </ws:ListBox.ItemContainerStyle>
            <ws:ListBoxItem Content="QWQ"/>
        </ws:ListBox>
    </Grid>
</Window>
